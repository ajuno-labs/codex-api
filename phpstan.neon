includes:
    - vendor/larastan/larastan/extension.neon

parameters:
    paths:
        - app/
        - bootstrap/
        - config/
        - database/
        - routes/
        - tests/

    # Level 9 is the highest level
    level: 5

    excludePaths:
        - */vendor/*
        - */node_modules/*
        - */storage/*
        - bootstrap/cache/* 